{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "ontology_iri": {
        "type": "string",
        "format": "uri"
      },
      "classes": {
        "type": "object",
        "additionalProperties": {
          "type": "string"
        }
      },
      "object_properties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "domain": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "range": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "inverse_property": {
              "type": "string"
            },
            "property_type": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["domain", "range"]
        }
      },
      "data_properties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "domain": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "range": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "property_type": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["domain", "range"]
        }
      },
      "individuals": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "class": {
              "type": "string"
            },
            "attributes": {
              "type": "object",
              "additionalProperties": {
                "type": "array"
              }
            }
          },
          "required": ["name", "class", "attributes"]
        }
      },
      "disjoint_classes": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "equivalent_classes": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "class": {
              "type": "string"
            },
            "equivalent_to": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["class", "equivalent_to"]
        }
      },
      "general_axioms": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "axiom_type": {
              "type": "string"
            },
            "properties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["axiom_type", "properties"]
        }
      }
    },
    "required": ["ontology_iri", "classes"]
  }